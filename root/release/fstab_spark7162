proc	          /proc           proc    defaults                        			0       0
tmpfs             /tmp            tmpfs   defaults                        			0       0
LABEL=RECORD      /hdd            auto    defaults,errors=remount-ro,noatime,nodiratime      	0       0
LABEL=SWAPPART    none            swap    sw

